[global_config]
  borderless = True
  extra_styling = False
  putty_paste_style = True
  title_font = Sans 10
  title_use_system_font = False
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#002b36"
    background_darkness = 0.45
    background_type = transparent
    cursor_color = "#aaaaaa"
    font = Monospace 10
    foreground_color = "#839496"
    icon_bell = False
    show_titlebar = False
    use_system_font = False
